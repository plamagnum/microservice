{
    "name": "yourvendor/notification-service",
    "description": "Notification Service for Microservices Example (RabbitMQ Consumer)",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "php-amqplib/php-amqplib": "^3.6",
        "monolog/monolog": "^3.5",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\NotificationService\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}